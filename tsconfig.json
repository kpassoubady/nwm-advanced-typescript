{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "noEmitOnError": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "noImplicitReturns": false,
    "target": "ES2021",
    "lib": ["ES2021","dom"], 
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ]
}
